{
  "Logging": {
    "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http.HttpClient": "Warning"
    },
    "Console": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System.Net.Http.HttpClient": "Warning"
        },
        "FormatterOptions": {
            "IncludeScopes": false,
            "TimestampFormat": "HH:mm:ss.ffff\t",
            "UseUtcTimestamp": true
        }
        //"FormatterName":  "Json"
    }
  }
}
